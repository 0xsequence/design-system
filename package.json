{
  "name": "@0xsequence/design-system",
  "version": "0.1.98",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.es.js",
  "module": "dist/index.cjs.js",
  "types": "dist/types/index.d.ts",
  "homepage": "https://0xsequence.github.io/design-system/",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./styles.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "start": "pnpm storybook",
    "build": "tsc && vite build",
    "build:icons": "pnpm svgr -d ./src/icons ./public/static/icons --config-file ./svgr.config.json --template ./src/icons/template.cjs",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build --no-manager-cache -o ./docs",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint -c .eslintrc './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint -c .eslintrc --fix './src/**/*.{ts,tsx}'",
    "format": "prettier --list-different --write src/**/*.{ts,tsx}",
    "prepare": "husky install",
    "prepack": "pnpm build",
    "patch": "pnpm version --patch && git push --follow-tags"
  },
  "dependencies": {
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-aspect-ratio": "^1.0.1",
    "@radix-ui/react-checkbox": "^1.0.1",
    "@radix-ui/react-collapsible": "^1.0.1",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-dropdown-menu": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.1",
    "@radix-ui/react-radio-group": "^1.1.1",
    "@radix-ui/react-select": "^1.2.0",
    "@radix-ui/react-switch": "^1.0.1",
    "@radix-ui/react-tabs": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.3",
    "react-blockies": "^1.4.1",
    "react-hook-form": "^7.40.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@jest/types": "^29.3.1",
    "@storybook/addon-actions": "^7.0.0-beta.13",
    "@storybook/addon-docs": "^7.0.0-beta.13",
    "@storybook/addon-essentials": "^7.0.0-beta.13",
    "@storybook/addon-links": "^7.0.0-beta.13",
    "@storybook/react": "^7.0.0-beta.13",
    "@storybook/react-vite": "^7.0.0-beta.13",
    "@storybook/theming": "^7.0.0-beta.13",
    "@svgr/cli": "^6.5.1",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-blockies": "^1.4.1",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vanilla-extract/css": "^1.9.2",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/jest-transform": "^1.0.1",
    "@vanilla-extract/recipes": "^0.3.0",
    "@vanilla-extract/sprinkles": "^1.5.1",
    "@vanilla-extract/vite-plugin": "^3.7.0",
    "@vitejs/plugin-react": "^3.0.1",
    "clsx": "^1.2.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "framer-motion": "^7.6.7",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.13",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.6.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.0.3"
  },
  "peerDependencies": {
    "framer-motion": ">=6",
    "react": ">=17",
    "react-dom": ">=17"
  }
}
